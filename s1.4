/////////////////////palin:

#include "pzhelp"  
#include <iostream>  
using namespace std;  
int main() {  
  int i,j,c,m;  
  bool a;  
  char pal[21],input;  
  REAL n;  
  n=READ_REAL();  
  c=0;  
  SKIP_LINE();  
  for(i=0;i<n;i++)  
  {  
    a=true;  
    m=0;  
    do  
    {  
      if(m==21)  
      {  
        SKIP_LINE();  
        break;  
      }  
      else   
      {  
        input=getchar();  
        pal[m]=input;  
        m=m+1;  
      }  
    }  
    while(input!='\n');  
    if(m==1)  
    {  
      cout<<"empty"<<endl;  
      continue;  
    }  
    else if(m==21 && input !='\n')  
    {  
      cout<<"error"<<endl;  
      continue;  
    }  
    else   
    {  
      m=m-1;  
      j=0;  
       do   
       {  
         if(pal[j]!=pal[m-j-1])  
           a=false;  
         else j=j+1;  
       }  
       while (a && j<m-j);  
    }  
    if (a)  
    {  
      cout<<"yes"<<endl;  
      c=c+1;  
    }  
    else cout<<"no"<<endl;  
  }  
  n=c*100.0/n;  
  WRITELN(FORM(n,0,3));  
}  

////////////////////////minmax:
#include "pzhelp"  
#include <iostream>  
PROGRAM {  
  int i,j,min,max,n,min1[100],m,max1[100];  
  n=READ_INT();  
  m=READ_INT();  
  float a[n][m];  
  for (i=0;i<n;i++)  
    for (j=0;j<m;j++) cin>>a[i][j];  
  for (j=0;j<m;j++)  
  {  
    max1[j] = a[0][j];  
    for (i=0;i<n;i++)  
      if (a[i][j] > max1[j]) max1[j] = a[i][j];  
  }  
  min=max1[0];  
  for (j=0;j<m;j++)  
  {  
    if (max1[j]<min) min=max1[j];  
  }  
  WRITELN(min);  
  for (i=0;i<n;i++)  
  {  
    min1[i] = a[i][0];  
    for (j=0;j<m;j++)  
      if (a[i][j]<min1[i])  min1[i]=a[i][j];  
  }  
  max=min1[0];  
  for (i=0;i<n;i++)  
  {  
    if (min1[i]>max) max=min1[i];  
  }  
WRITELN(max);  
}




